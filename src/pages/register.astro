---
import Register from "../components/register/Register.astro";
import BaseLayout from "../layouts/BaseLayout.astro";

import { checkSession } from "../utils/checkSession";

const cookie = Astro.request.headers.get("cookie");
const isLoggedin = checkSession(cookie);
---

<BaseLayout title="SKF | Register" loadNav={false} isLoggedin={isLoggedin}>
  <Register />
</BaseLayout>

<style>
  :global(html, body) {
    height: 100%;
  }
</style>
